    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.86",
    "@types/react": "^18.2.63",
    "@types/react-dom": "^18.2.20",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "react-icons": "^5.0.1",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.13.8",
